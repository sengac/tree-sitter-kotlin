{
  "name": "@sengac/tree-sitter-kotlin",
  "version": "0.4.4",
  "description": "Kotlin grammar for tree-sitter",
  "repository": "https://github.com/sengac/tree-sitter-kotlin",
  "license": "MIT",
  "author": {
    "name": "Amaan Qureshi",
    "email": "amaanq12@gmail.com"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "kotlin"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.3.0",
    "node-gyp-build": "^4.8.4",
    "npm-check-updates": "^17.1.13"
  },
  "devDependencies": {
    "eslint": "^9.17.0",
    "eslint-config-treesitter": "^1.0.2",
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.24.6"
  },
  "peerDependencies": {
    "@sengac/tree-sitter": "^0.25.10"
  },
  "peerDependenciesMeta": {
    "@sengac/tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "lint": "eslint grammar.js",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js",
    "install": "node-gyp-build"
  },
  "publishConfig": {
    "access": "public"
  }
}
